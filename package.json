    "@types/react-dom": "18.0.9",
    "eslint": "8.28.0",
    "eslint-config-next": "^13.0.4",
    "next": "^13.0.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "typescript": "4.9.3"
  },
  "devDependencies": {
    "autoprefixer": "^10.4.13",
    "concurrently": "^7.6.0",
    "postcss": "^8.4.19",
    "tailwindcss": "^3.2.4"
  }
